{
  "kind": "collectionType",
  "collectionName": "blog_posts",
  "info": {
    "name": "Blog post"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Content": {
      "type": "richtext"
    },
    "comments": {
      "plugin": "comments",
      "collection": "comment",
      "via": "related"
    },
    "navigation": {
      "model": "navigationitem",
      "plugin": "navigation",
      "via": "related",
      "configurable": false,
      "hidden": true
    }
  }
}
